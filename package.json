{
  "name": "svgr-component-generator",
  "version": "1.0.3",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint .",
    "flow": "flow check",
    "clean": "rimraf lib",
    "flowbuild": "flow-copy-source src lib",
    "prebuild": "npm run clean && npm run flowbuild",
    "build": "babel src -d lib",
    "preversion": "npm run lint && npm test && npm run build",
    "version": "standard-changelog && git add CHANGELOG.md",
    "postpublish": "git push origin master --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrius-skucas-db/svgr-component-generator.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andrius-skucas-db/svgr-component-generator/issues"
  },
  "homepage": "https://github.com/andrius-skucas-db/svgr-component-generator#readme",
  "bin": {
    "svgr-generate-components": "./bin/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.4.0",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/preset-env": "7.4.2",
    "@babel/preset-flow": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.5.0",
    "documentation": "9.3.1",
    "eslint": "5.15.3",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-flowtype": "3.4.2",
    "eslint-plugin-flowtype-errors": "4.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-prettier": "3.0.1",
    "flow-bin": "0.95.1",
    "flow-copy-source": "2.0.3",
    "husky": "1.3.1",
    "jest": "24.5.0",
    "lint-staged": "8.1.5",
    "opn-cli": "4.0.0",
    "prettier": "1.16.4",
    "rimraf": "2.6.3",
    "standard-changelog": "2.0.7"
  }
}
